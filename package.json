{
  "name": "javascript-af-ui",
  "version": "0.0.20",
  "description": "UI kit for javascript.af",
  "browser": "./umd/jsui.umd.production.js",
  "main": "./index.js",
  "repository": "https://github.com/javascript-af/ui-lib.git",
  "author": "Harshit Pant <pantharshit00@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javascript-af/ui-lib/issues"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "private": false,
  "scripts": {
    "precommit": "lint-staged && yarn run test",
    "build:umd": "cross-env NODE_ENV=production rollup -c",
    "prebuild": "rimraf dist/",
    "copyfiles": "cp README.md dist/ && cp LICENSE dist/ && cp package.json dist/",
    "build": "yarn run build:modules && yarn run build:umd && yarn run copyfiles",
    "build:modules": "tsc",
    "dev": "docz dev",
    "build:docz": "docz build",
    "prerelease": "yarn build",
    "release": "npm publish dist"
  },
  "dependencies": {
    "@atlaskit/blanket": "^5.0.2",
    "@atlaskit/spinner": "^5.0.2",
    "classnames": "^2.2.5",
    "docz-theme-default": "^0.2.3"
  },
  "peerDependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "3.3.2"
  },
  "resolutions": {
    "styled-components": "3.3.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/node": "^10.3.2",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "babel-core": "^6.26.3",
    "babel-preset-es2015-rollup": "^3.0.0",
    "cross-env": "^5.1.6",
    "docz": "^0.2.3",
    "flow-bin": "^0.72.0",
    "prettier": "^1.12.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.1"
  }
}
