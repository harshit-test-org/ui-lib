{
  "name": "javascript-af-ui",
  "version": "0.0.2",
  "description": "UI kit for javascript.af",
  "browser": "./umd/jsui.umd.production.js",
  "main": "./index.js",
  "repository": "https://github.com/javascript-af/ui-lib.git",
  "author": "Harshit Pant <pantharshit00@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/javascript-af/ui-lib/issues"
  },
  "keywords": ["react", "react-component"],
  "private": false,
  "scripts": {
    "precommit": "lint-staged && yarn run test",
    "prettier": "prettier --config ./.prettierrc --write",
    "lint": "eslint components",
    "develop": "start-storybook -p 9001 -c .storybook",
    "build:storybook": "build-storybook -c .storybook -o build",
    "build:umd": "cross-env NODE_ENV=production rollup -c",
    "prebuild": "rimraf dist/",
    "copyfiles": "cp README.md dist/ && cp LICENSE dist/ && cp package.json dist/",
    "build": "yarn run build:umd && yarn run build:modules && yarn run copyfiles & yarn run build:storybook",
    "build:modules":
      "cross-env BABEL_ENV=production babel --plugins transform-es2015-modules-commonjs components/ -d dist/ --ignore *.stories.js",
    "test": "jest",
    "prerelease": "yarn run lint && yarn run test",
    "release": "lerna publish"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addons": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "^3.1.1",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.8.2",
    "eslint-plugin-standard": "^3.1.0",
    "flow-bin": "^0.72.0",
    "lerna": "^2.11.0",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-ignore": "^1.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^4.0.0"
  }
}
